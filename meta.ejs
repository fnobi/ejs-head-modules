<!-- meta -->
<meta charset="utf-8" /><% if (meta.description) { %>
<meta name="description" content="<%= meta.description %>" /><% } %><% if (meta.keywords.length) { %>
<meta name="keywords" content="<%= meta.keywords.join(', ') %>" /><% } %><% if (meta.favicon_url) { %>
<link rel="shortcut icon" href="<%= meta.favicon_url %>" type="image/vnd.microsoft.icon" />
<link rel="icon" href="<%= meta.favicon_url %>" type="image/vnd.microsoft.icon" /><% } %>
