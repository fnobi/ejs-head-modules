<!-- ios-meta -->
<meta name="viewport" content="<%= ios.viewport.join(', ') %>" />
<meta name="apple-mobile-web-app-capable" content="<%= ios.capable ? 'yes' : 'no' %>">
<meta name="apple-mobile-web-app-status-bar-style" content="<%= ios.status_bar %>">
<link rel="apple-touch-icon" href="<%= ios.icon_url %>"/>
